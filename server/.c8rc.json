{
  "all": true,
  "include": ["src/**/*.js"],
  "exclude": [
    "src/**/*.test.js", 
    "src/test/**/*.js", 
    "src/__tests__/**/*.js", 
    "test-*.js",
    "src/constants/index.js",
    "src/config/server-config.js",
    "src/index.js",
    "src/config/server-startup.js",
    "src/config/tls-config.js",
    "src/services/claude-code.js"
  ],
  "reporter": ["text", "lcov", "html"],
  "check-coverage": false,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
