{
  "all": true,
  "include": ["src/**/*.js"],
  "exclude": ["src/**/*.test.js", "src/test/**/*.js", "src/__tests__/**/*.js", "test-*.js"],
  "reporter": ["text", "lcov", "html"],
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
